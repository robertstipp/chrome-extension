{
  "manifest_version": 3,
  "name": "Element Inspector",
  "version": "1.0",
  "action" : {
    "default_title": "Click to show an alert"
  },
  "permissions" : [
    "tabs", 
    "windows",
    "activeTab",
    "tabGroups",
    "scripting",
    "storage"

  ],
  "background" : {
    "service_worker": "background.js"
  }
}